{
  "name": "chrome-extension",
  "version": "1.0.0",
  "description": "Chrome extension for improved referencing in GitHub",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "sass --load-path=node_modules/ src/styles/index.scss:dist/css/index.css",
    "scss-watch": "sass --watch --load-path=node_modules/ src/styles/index.scss:dist/css/index.css",
    "start": "webpack --watch --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js && npm run scss"
  },
  "private": "true",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@primer/css": "^19.0.0",
    "@primer/primitives": "^7.1.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "eslint": "^8.6.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  }
}
