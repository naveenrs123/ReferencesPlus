{
  "name": "chrome-extension",
  "version": "1.0.0",
  "description": "Chrome extension for improved referencing in GitHub",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "sass --load-path=node_modules/ src/styles/index.scss:dist/css/index.css",
    "scss-watch": "sass --watch --load-path=node_modules/ src/styles/index.scss:dist/css/index.css",
    "start": "webpack --watch --config webpack/webpack.dev.js && npm run scss",
    "build": "webpack --config webpack/webpack.prod.js && npm run scss"
  },
  "private": "true",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@primer/css": "^19.0.0",
    "@primer/primitives": "^7.1.0",
    "@types/chrome": "^0.0.177",
    "buffer": "^6.0.3",
    "rrweb": "^1.1.1",
    "rrweb-player": "^0.7.12",
    "rrweb-snapshot": "^1.1.12",
    "ts-ebml": "^2.0.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "eslint": "^8.6.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  }
}
