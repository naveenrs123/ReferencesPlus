{
    "name": "References+ for GitHub",
    "description": "An extension that provides greater support for referencing in GitHub Pull Requests.",
    "version": "1.0.0",
    "manifest_version": 3,
    "key": "",
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": ["storage", "identity"],
    "action": {
        "default_popup": "templates/popup.html"
    },
    "oauth2": {
        "client_id": "91a16e0d9c142eeac319",
        "scopes": [""]
    },
    "content_scripts": [
        {
            "matches": ["http://127.0.0.1:5000/*"],
            "js":["scripts/content-script.js"]
        }
    ]
}