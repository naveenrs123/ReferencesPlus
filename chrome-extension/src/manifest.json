{
    "name": "References+ for GitHub",
    "description": "An extension that provides greater support for referencing in GitHub Pull Requests.",
    "version": "1.0.0",
    "manifest_version": 3,
    "key": "",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "identity", "tabs", "webNavigation", "contextMenus"],
    "action": {
        "default_icon": {
            "16": "static/favicon-16x16.png",
            "32": "static/favicon-32x32.png",
            "48": "static/favicon-32x32.png",
            "128": "static/favicon-32x32.png"
        }
    },
    "icons": {
        "16": "static/favicon-16x16.png",
        "32": "static/favicon-32x32.png",
        "48": "static/favicon-32x32.png",
        "128": "static/favicon-32x32.png"
    },
    "oauth2": {
        "client_id": "91a16e0d9c142eeac319",
        "scopes": [""]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js":["content.js"]
        }
    ]
}